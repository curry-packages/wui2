{
  "name": "wui2",
  "version": "4.0.0",
  "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
  "synopsis": "Libraries to support the type-oriented construction of web user interfaces based on the html2 package",
  "category": [ "User Interface", "Web" ],
  "license": "BSD-3-Clause",
  "licenseFile": "LICENSE",
  "dependencies": {
    "base"       : " >= 3.0.0, < 4.0.0",
    "html2"      : " >= 4.0.0, < 5.0.0",
    "inversion"  : " >= 3.0.0, < 4.0.0",
    "time"       : " >= 3.0.0, < 4.0.0"
  },
  "source": {
    "git": "https://github.com/curry-packages/wui2.git",
    "tag": "$version"
  },
  "exportedModules": [ "HTML.WUI" ],
  "testsuite": [ {
                 "src-dir": "src",
                 "options": "-v --nosource",
                 "modules": [ "HTML.WUI" ]
               }
               , {
                 "src-dir": "examples",
                 "options": "-v",
                 "modules": [ "Persons" ]
               } ]
}
