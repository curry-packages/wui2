{
  "name": "wui2",
  "version": "0.0.1",
  "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
  "synopsis": "Libraries to support the type-oriented construction of web user interfaces based on the html2 package",
  "category": [ "User Interface", "Web" ],
  "license": "BSD-3-Clause",
  "licenseFile": "LICENSE",
  "dependencies": {
    "base" : " >= 1.0.0,  < 2.0.0",
    "html2": " >= 0.0.1, < 1.0.0"
  },
  "compilerCompatibility": {
    "pakcs": " >= 2.0.0,  < 3.0.0",
    "kics2": " >= 2.0.0,  < 3.0.0"
  },
  "source": {
    "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/wui2.git",
    "tag": "$version"
  },
  "exportedModules": [ "HTML.WUI" ],
  "testsuite": [ {
                 "src-dir": "src",
                 "options": "-v --nosource",
                 "modules": [ "HTML.WUI" ]
               }
               , {
                 "src-dir": "examples",
                 "options": "-v",
                 "modules": [ "Persons" ]
               } ]
}
